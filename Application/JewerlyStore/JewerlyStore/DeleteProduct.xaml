<Window x:Class="JewerlyStore.DeleteProduct"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JewerlyStore"
        mc:Ignorable="d"
        Title="DeleteProduct" Height="190" Width="300">
    <Grid Background="#FFEA6F6F">
        <Label VerticalAlignment="Top" HorizontalAlignment="Center" FontSize="16" Margin="0 30 0 0">Enter Product Id</Label>
        <TextBox x:Name="delProductBox" VerticalAlignment="Top" HorizontalAlignment="Center" Width="100" Height="24" Margin="0 60 0 0"></TextBox>
        <Button VerticalAlignment="Top" HorizontalAlignment="Center" Width="60" Height="24" Margin="0 110 0 0" FontSize="14" Background="#FF3D8CDC" Click="delProductClick">Delete</Button>
    </Grid>
</Window>
